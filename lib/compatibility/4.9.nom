#!/usr/bin/env nomsu -V6.15.13.8
#
    This file defines upgrades from Nomsu <4.9 to 4.9
use "compatibility/compatibility"

~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~

upgrade action "if" to "4.9" via
    for ($tree $end_version):
        if ((size of $tree) > 2): return $tree
        return \(when $tree.2)
